{
  "name": "sealos",
  "private": "true",
  "version": "0.1.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "gen:theme-typings": "pnpm chakra-cli tokens packages/ui/src/theme/theme.ts --out node_modules/.pnpm/node_modules/@chakra-ui/styled-system/dist/theming.types.d.ts",
		"gen:admin-theme-typings": "pnpm chakra-cli tokens admin/src/styles/index.ts --out node_modules/.pnpm/node_modules/@chakra-ui/styled-system/dist/theming.types.d.ts",
    "gen:sdk": "pnpm -r --filter ./packages/client-sdk run build",
    "postinstall": "pnpm run gen:theme-typings && pnpm run gen:sdk",
    "prepare": "husky",
		"fmt": "prettier --write --ignore-unknown admin/",
    "test": "pnpm -r --filter./admin/* run test"
  },
  "workspaces": [
    "./packages/*",
    "./admin"
  ],
  "devDependencies": {
    "@chakra-ui/cli": "^2.4.1",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "prettier": "^2.8.8"
  },
  "lint-staged": {
    "admin/*.{ts,tsx}": [
      "eslint --max-warnings 0 ."
    ],
    "**/*": "prettier --write --ignore-unknown admin/"
  },
  "dependencies": {
    "@chakra-ui/anatomy": "^2.2.1",
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/next-js": "^2.1.5",
    "@chakra-ui/react": "^2.8.1",
    "@chakra-ui/system": "^2.6.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "framer-motion": "^10.16.4",
    "i18next": "^23.11.5",
    "next-i18next": "^15.3.0",
    "react-i18next": "^14.1.2",
    "typescript": "~5.5.0"
  },
  "pnpm": {
    "supportedArchitectures": {
      "os": [
        "win32",
        "darwin",
        "current",
        "linuxmusl"
      ],
      "cpu": [
        "x64",
        "arm64"
      ],
      "libc": [
        "musl"
      ]
    }
  }
}
